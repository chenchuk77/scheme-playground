   "source": [
    "; check if element in collection\n",
    "(define (in e c) \n",
    "  (if (eq? c '()) \n",
    "      #f \n",
    "      (if (eq? e (car c)) \n",
    "          #t \n",
    "          (in e (cdr c)))\n",
    "      ))\n",
    "\n"
   ]
  },
